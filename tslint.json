{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark" : [
      true,
      "single"
    ],
    "semicolon": [true, "never"],
    "indent": [false, "tabs", 2],
    "object-literal-sort-keys": false,
    "object-literal-key-quotes": false,
    "trailing-comma": false,
    "no-trailing-whitespace": false,
    "eofline": false,
    "array-type": false,
    "import-sources-order": "any"
  },
  "rulesDirectory": []
}